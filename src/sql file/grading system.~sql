--CUSTOMER TABLE
create table CUSTOMER_TAB(CID_PK NUMBER(8),CNAME VARCHAR2(100),CADD VARCHAR2(50),
C_CONTACT NUMBER(10),C_CREDITDAYS NUMBER(4),CJ_DATE DATE,SEX VARCHAR2(10));  

ALTER TABLE CUSTOMER_TAB MODIFY CID_PK NUMBER(8) PRIMARY KEY; 

