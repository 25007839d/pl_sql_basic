--create table ser_det


create table ser_det_tab (sid_pk number(8) primary key,cid_fk number(8)references CUSTOMER_TAB(CID_PK),
eid_fk number(8)references employee_tab3(eid_pk),spid_FK NUMBER(8)references sparepart_tab(spid_pk),
typ_veh number(8),veh_no number(8),typ_ser number(8),ser_date date,qty number(8),sp_rate number(8),
sp_amt number(8),sp_g number(8),ser_amt number(8),comm number(8),total number(8)); 

