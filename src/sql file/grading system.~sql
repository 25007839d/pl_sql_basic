-- PURCHASE TABLE

CREATE TABLE PURCHASE_TAB (PID_PK NUMBER(8)PRIMARY KEY,
VID_FK NUMBER(8) REFERENCES VENDOR_TAB(VID_PK),
SPID_FK NUMBER(8)REFERENCES SPAREPART_TAB (SPID_PK),PQTY VARCHAR2(8), SPRAT VARCHAR2(8),
SPGST VARCHAR2(8), PDATE NUMBER(8), TRANCOST NUMBER(8), TOTAL NUMBER(8),
EID NUMBER(8)REFERENCES EMPLOYEE_TAB3 (EID_PK));

SELECT * FROM PURCHASE_TAB

